#!/usr/bin/env node
'use strict';

// var subcommand = process.argv;
// console.log(subcommand);
// var argv = require('minimist')(process.argv.slice(2));
// console.dir(argv);
// process.exit();

var program = require('commander');

program
  .version('0.0.1')
  .command('new', 'create a new project')
  .command('deploy', 'deploy the application')
  .command('inspect-api', 'displays the OpenAPI specification of an API')
  .command('inspect-endpoint', 'displays the OpenAPI specification of an endpoint')
  .parse(process.argv);
